<Window x:Class="myPTv1._0.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="600" Width="1300" Loaded="Window_Loaded_1" Unloaded="Window_Unloaded_1">
    <Grid IsEnabledChanged="Grid_IsEnabledChanged_1" Unloaded="Grid_Unloaded_1">
        <Image Height="480" HorizontalAlignment="Left" Margin="0,20,0,0" Name="image1" Stretch="Fill" VerticalAlignment="Top" Width="640" />
        <Canvas HorizontalAlignment="Left" Height="480" Margin="655,20,-3,0" Name="canvas1" VerticalAlignment="Top" Width="640">
            <Ellipse Canvas.Left="0" Canvas.Top="0" Height="20" Name="kneeEllipse" Stroke="Black" Width="20" Fill="Orange" />
            <Ellipse Canvas.Left="50" Canvas.Top="0" Height="20" Name="ankleEllipse" Stroke="Black" Width="20" Fill="SlateGray" />
            <Ellipse Canvas.Left="100" Canvas.Top="0" Fill="SpringGreen" Height="20" Name="thighEllipse" Stroke="Black" Width="20" />
            <Line Stroke="Black" StrokeThickness="8" Name="femur"/>
            <Line Stroke="Black" StrokeThickness="8" Name="tibia"/>
            <Label Canvas.Left="315" Canvas.Top="27" Name="AngleText"/>
        </Canvas>
    </Grid>
</Window>